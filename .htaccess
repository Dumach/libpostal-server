RewriteEngine On

# Route /parse to index.php
RewriteRule ^parse$ index.php [L,QSA]

# Route /parse/ to index.php (optional trailing slash)
RewriteRule ^parse/$ index.php [L,QSA]

# Handle existing files and directories normally
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d